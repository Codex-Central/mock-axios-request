{
  "compilerOptions": {
    "lib": ["es2020", "dom"],
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "outDir": "dist",
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
